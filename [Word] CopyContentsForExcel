Sub CopyContentsForExcel()
'
'
'


'----------------------------
'포토샵 자막 작업용 추출 시작
'----------------------------

    '포토샵 자막 모음 다름 이름으로 저장
    Dim docName As String
    docName = Left(ActiveDocument.FullName, InStrRev(ActiveDocument.FullName, ".") - 1)

    Selection.WholeStory
    Selection.Copy
    Documents.Add Template:="Normal", NewTemplate:=False, DocumentType:=0
    Selection.PasteAndFormat (wdPasteDefault)
    ActiveDocument.SaveAs FileName:=docName & "_포토샵 자막"



    '수식 변환
    Dim MathObj As Object
        For Each MathObj In ActiveDocument.OMaths
        With MathObj
        .BuildUp


        End With
        Next

    '여백 조절
    With Selection.PageSetup
        .LineNumbering.Active = False
        .Orientation = wdOrientPortrait
        .TopMargin = CentimetersToPoints(1.27)
        .BottomMargin = CentimetersToPoints(1.27)
        .LeftMargin = CentimetersToPoints(1.27)
        .RightMargin = CentimetersToPoints(1.27)
        .Gutter = CentimetersToPoints(0)
        .HeaderDistance = CentimetersToPoints(1.5)
        .FooterDistance = CentimetersToPoints(1.75)
        .PageWidth = CentimetersToPoints(21)
        .PageHeight = CentimetersToPoints(29.7)
        .FirstPageTray = wdPrinterDefaultBin
        .OtherPagesTray = wdPrinterDefaultBin
        .SectionStart = wdSectionNewPage
        .OddAndEvenPagesHeaderFooter = False
        .DifferentFirstPageHeaderFooter = False
        .VerticalAlignment = wdAlignVerticalTop
        .SuppressEndnotes = False
        .MirrorMargins = False
        .TwoPagesOnOne = False
        .BookFoldPrinting = False
        .BookFoldRevPrinting = False
        .BookFoldPrintingSheets = 1
        .GutterPos = wdGutterPosLeft
        .CharsLine = 45
        .LinesPage = 38
        .LayoutMode = wdLayoutModeDefault
    End With

    '테이블 여백조절&정리
    ActiveDocument.Tables(1).Cell(9, 1).SetWidth _
    ColumnWidth:=20, RulerStyle:=wdAdjustProportional
    ActiveDocument.Tables(1).Cell(9, 2).SetWidth _
    ColumnWidth:=20, RulerStyle:=wdAdjustProportional
    ActiveDocument.Tables(1).Cell(10, 2).SetWidth _
    ColumnWidth:=20, RulerStyle:=wdAdjustProportional
    ActiveDocument.Tables(1).Cell(11, 2).SetWidth _
    ColumnWidth:=20, RulerStyle:=wdAdjustProportional
    ActiveDocument.Tables(1).Cell(12, 2).SetWidth _
    ColumnWidth:=20, RulerStyle:=wdAdjustProportional
    ActiveDocument.Tables(1).Cell(13, 2).SetWidth _
    ColumnWidth:=20, RulerStyle:=wdAdjustProportional
    ActiveDocument.Tables(1).Cell(14, 2).SetWidth _
    ColumnWidth:=20, RulerStyle:=wdAdjustProportional
    ActiveDocument.Tables(1).Cell(Row:=9, Column:=2).Select
    Selection.Delete

    ActiveDocument.Tables(1).Cell(Row:=9, Column:=3).Select
    Selection.Delete

    ActiveDocument.Tables(1).Cell(Row:=10, Column:=2).Select
    Selection.Delete

    ActiveDocument.Tables(1).Cell(Row:=10, Column:=3).Select
    Selection.Delete

    ActiveDocument.Tables(1).Cell(Row:=12, Column:=2).Select
    Selection.Delete

    ActiveDocument.Tables(1).Cell(Row:=12, Column:=3).Select
    Selection.Delete

    ActiveDocument.Tables(1).Cell(Row:=13, Column:=2).Select
    Selection.Delete

    ActiveDocument.Tables(1).Cell(Row:=13, Column:=3).Select
    Selection.Delete

    ActiveDocument.Tables(1).Cell(Row:=14, Column:=2).Select
    Selection.Delete

    ActiveDocument.Tables(1).Cell(Row:=14, Column:=3).Select
    Selection.Delete

    ActiveDocument.Tables(1).Cell(Row:=8, Column:=1).Delete
    ActiveDocument.Tables(1).Cell(Row:=4, Column:=1).Delete
    ActiveDocument.Tables(1).Cell(Row:=2, Column:=1).Delete
    ActiveDocument.Tables(1).Cell(Row:=1, Column:=1).Delete

    '타임코드-자막 사이 공백 없을 시 공백 삽입
    ActiveDocument.Tables(1).Cell(Row:=7, Column:=3).Range.Select
    Selection.Find.ClearFormatting
    Selection.Find.Replacement.ClearFormatting
    With Selection.Find
        .Text = "([0-9][0-9]:[0-9][0-9])([! ])"
        .Replacement.Text = "\1 \2"
        .Forward = True
        .Wrap = wdFindContinue
        .Format = False
        .MatchCase = False
        .MatchWholeWord = False
        .MatchByte = False
        .CorrectHangulEndings = False
        .HanjaPhoneticHangul = False
        .MatchAllWordForms = False
        .MatchSoundsLike = False
        .MatchWildcards = True
    End With
    Selection.Find.Execute Replace:=wdReplaceAll

    '폰트 및 줄간격 변경 (가독성+)
    Selection.WholeStory
    Selection.ParagraphFormat.LineSpacing = LinesToPoints(1)
    Selection.Font.Name = "맑은 고딕"


    '타임코드-자막 사이 너무 넓은 공백 맞춤
    ActiveDocument.Tables(1).Cell(Row:=7, Column:=3).Range.Select
    Selection.Find.ClearFormatting
    Selection.Find.Replacement.ClearFormatting
    With Selection.Find
        .Text = "([0-9][0-9]:[0-9][0-9])( {2,10})(\[?{1,4}\])"
        .Replacement.Text = "\1 \3"
        .Forward = True
        .Wrap = wdFindContinue
        .Format = False
        .MatchCase = False
        .MatchWholeWord = False
        .MatchByte = False
        .CorrectHangulEndings = False
        .HanjaPhoneticHangul = False
        .MatchAllWordForms = False
        .MatchSoundsLike = False
        .MatchWildcards = True
    End With
    Selection.Find.Execute Replace:=wdReplaceAll

    '필터 시작전
    ActiveDocument.Tables(1).Cell(Row:=7, Column:=3).Range.Select
    Selection.HomeKey Unit:=wdLine
    Selection.TypeParagraph
    ActiveDocument.Tables(1).Cell(Row:=7, Column:=3).Range.Select
    Selection.EndKey Unit:=wdLine
    Selection.TypeParagraph
    Selection.TypeText Text:="/!----"


    '필터 시작
    ActiveDocument.Tables(1).Cell(Row:=7, Column:=3).Range.Select
    Selection.Find.ClearFormatting
    Selection.Find.Replacement.ClearFormatting
    With Selection.Find
        .Text = _
            "(^13)([0-9]\-[0-9]?@[0-9]\.mp4^13)"
        .Replacement.Text = "\199:99 \2"
        .Forward = True
        .Wrap = wdFindContinue
        .Format = False
        .MatchCase = False
        .MatchWholeWord = False
        .MatchByte = False
        .CorrectHangulEndings = False
        .HanjaPhoneticHangul = False
        .MatchAllWordForms = False
        .MatchSoundsLike = False
        .MatchWildcards = True
    End With
    Selection.Find.Execute Replace:=wdReplaceAll

    ActiveDocument.Tables(1).Cell(Row:=7, Column:=3).Range.Select
    Selection.Find.ClearFormatting
    Selection.Find.Replacement.ClearFormatting
    With Selection.Find
        .Text = _
            "(^13[0-6][0-9]:[0-9][0-9] \[?{1,4}\])"
        .Replacement.Text = "^13/!----\1"
        .Forward = True
        .Wrap = wdFindContinue
        .Format = False
        .MatchCase = False
        .MatchWholeWord = False
        .MatchByte = False
        .CorrectHangulEndings = False
        .HanjaPhoneticHangul = False
        .MatchAllWordForms = False
        .MatchSoundsLike = False
        .MatchWildcards = True
    End With
    Selection.Find.Execute Replace:=wdReplaceAll


    ActiveDocument.Tables(1).Cell(Row:=7, Column:=3).Range.Select
    Selection.Find.ClearFormatting
    Selection.Find.Replacement.ClearFormatting
    With Selection.Find
        .Text = "(99:99)"
        .Replacement.Text = "/!---- \1"
        .Forward = True
        .Wrap = wdFindContinue
        .Format = False
        .MatchCase = False
        .MatchWholeWord = False
        .MatchByte = False
        .CorrectHangulEndings = False
        .HanjaPhoneticHangul = False
        .MatchAllWordForms = False
        .MatchSoundsLike = False
        .MatchWildcards = True
    End With
    Selection.Find.Execute Replace:=wdReplaceAll

    ActiveDocument.Tables(1).Cell(Row:=7, Column:=3).Range.Select
    Selection.Find.ClearFormatting
    Selection.Find.Replacement.ClearFormatting
    With Selection.Find
        .Text = "(^13[0-6][0-9]:[0-9][0-9] [!\[])?@(\/\!----)"
        .Replacement.Text = "\2"
        .Forward = True
        .Wrap = wdFindContinue
        .Format = False
        .MatchCase = False
        .MatchWholeWord = False
        .MatchByte = False
        .CorrectHangulEndings = False
        .HanjaPhoneticHangul = False
        .MatchAllWordForms = False
        .MatchSoundsLike = False
        .MatchWildcards = True
    End With
    Selection.Find.Execute Replace:=wdReplaceAll

    ActiveDocument.Tables(1).Cell(Row:=7, Column:=3).Range.Select
    Selection.Find.ClearFormatting
    Selection.Find.Replacement.ClearFormatting
    With Selection.Find
        .Text = "(\/\!----)"
        .Replacement.Text = "^13"
        .Forward = True
        .Wrap = wdFindContinue
        .Format = False
        .MatchCase = False
        .MatchWholeWord = False
        .MatchByte = False
        .CorrectHangulEndings = False
        .HanjaPhoneticHangul = False
        .MatchAllWordForms = False
        .MatchSoundsLike = False
        .MatchWildcards = True
    End With
    Selection.Find.Execute Replace:=wdReplaceAll

    ActiveDocument.Tables(1).Cell(Row:=7, Column:=3).Range.Select
    Selection.Find.ClearFormatting
    Selection.Find.Replacement.ClearFormatting
    With Selection.Find
        .Text = "(99:99 )"
        .Replacement.Text = "^13"
        .Forward = True
        .Wrap = wdFindContinue
        .Format = False
        .MatchCase = False
        .MatchWholeWord = False
        .MatchByte = False
        .CorrectHangulEndings = False
        .HanjaPhoneticHangul = False
        .MatchAllWordForms = False
        .MatchSoundsLike = False
        .MatchWildcards = True
    End With
    Selection.Find.Execute Replace:=wdReplaceAll

    ActiveDocument.Tables(1).Cell(Row:=7, Column:=3).Range.Select
    Selection.Find.ClearFormatting
    Selection.Find.Replacement.ClearFormatting
    With Selection.Find
        .Text = "^13-@^13"
        .Replacement.Text = "^13"
        .Forward = True
        .Wrap = wdFindContinue
        .Format = False
        .MatchCase = False
        .MatchWholeWord = False
        .MatchByte = False
        .CorrectHangulEndings = False
        .HanjaPhoneticHangul = False
        .MatchAllWordForms = False
        .MatchSoundsLike = False
        .MatchWildcards = True
    End With
    Selection.Find.Execute Replace:=wdReplaceAll

    ActiveDocument.Tables(1).Cell(Row:=7, Column:=3).Range.Select
    Selection.Find.ClearFormatting
    Selection.Find.Replacement.ClearFormatting
    With Selection.Find
        .Text = "^13{2,}"
        .Replacement.Text = "^p"
        .Forward = True
        .Wrap = wdFindContinue
        .Format = False
        .MatchCase = False
        .MatchWholeWord = False
        .MatchByte = False
        .CorrectHangulEndings = False
        .HanjaPhoneticHangul = False
        .MatchAllWordForms = False
        .MatchSoundsLike = False
        .MatchWildcards = True
    End With
    Selection.Find.Execute Replace:=wdReplaceAll

    ActiveDocument.Tables(1).Cell(Row:=7, Column:=3).Range.Select
    Selection.Find.ClearFormatting
    Selection.Find.Replacement.ClearFormatting
    With Selection.Find
        .Text = "^13 ^13"
        .Replacement.Text = "^13"
        .Forward = True
        .Wrap = wdFindContinue
        .Format = False
        .MatchCase = False
        .MatchWholeWord = False
        .MatchByte = False
        .CorrectHangulEndings = False
        .HanjaPhoneticHangul = False
        .MatchAllWordForms = False
        .MatchSoundsLike = False
        .MatchWildcards = True
    End With
    Selection.Find.Execute Replace:=wdReplaceAll

    ActiveDocument.Tables(1).Cell(Row:=7, Column:=3).Range.Select
    Selection.Find.ClearFormatting
    Selection.Find.Replacement.ClearFormatting
    With Selection.Find
        .Text = "^13^13"
        .Replacement.Text = "^13"
        .Forward = True
        .Wrap = wdFindContinue
        .Format = False
        .MatchCase = False
        .MatchWholeWord = False
        .MatchByte = False
        .CorrectHangulEndings = False
        .HanjaPhoneticHangul = False
        .MatchAllWordForms = False
        .MatchSoundsLike = False
        .MatchWildcards = True
    End With
    Selection.Find.Execute Replace:=wdReplaceAll




    '저장 후 닫기
    ActiveDocument.Save
    ActiveDocument.Close
'--------------------------
'포토샵 자막 추출 작업 완료
'--------------------------


'-----------------------
'엑셀 붙혀넣기 작업 시작
'-----------------------


    '수식(분수등)을 일반 텍스트로 변환
    Dim MathObjForExcel As Object
    For Each MathObjForExcel In ActiveDocument.OMaths
    With MathObjForExcel
    .Remove
    End With
    Next

    '수식용 특수문자 제거1
    Selection.Find.ClearFormatting
    Selection.Find.Replacement.ClearFormatting
    With Selection.Find
        .Text = ChrW(12310)
        .Replacement.Text = ""
        .Forward = True
        .Wrap = wdFindContinue
        .Format = False
        .MatchCase = False
        .MatchWholeWord = False
        .MatchByte = False
        .CorrectHangulEndings = False
        .HanjaPhoneticHangul = False
        .MatchWildcards = False
        .MatchSoundsLike = False
        .MatchAllWordForms = False
    End With
    Selection.Find.Execute Replace:=wdReplaceAll

    '수식용 특수문자 제거2
    Selection.Find.ClearFormatting
    Selection.Find.Replacement.ClearFormatting
    With Selection.Find
        .Text = ChrW(12311)
        .Replacement.Text = ""
        .Forward = True
        .Wrap = wdFindContinue
        .Format = False
        .MatchCase = False
        .MatchWholeWord = False
        .MatchByte = False
        .CorrectHangulEndings = False
        .HanjaPhoneticHangul = False
        .MatchWildcards = False
        .MatchSoundsLike = False
        .MatchAllWordForms = False
    End With
    Selection.Find.Execute Replace:=wdReplaceAll

    '표를 텍스트로 변환
    Dim oTable As Table

    For Each oTable In ActiveDocument.Tables(1).Cell(Row:=11, Column:=3).Tables

        oTable.ConvertToText Separator:=wdSeparateByTabs, NestedTables:= _
        True

    Next


    '폰트 및 줄간격 변경 (가독성+)
    Selection.WholeStory
    Selection.ParagraphFormat.LineSpacing = LinesToPoints(1)
    Selection.Font.Name = "맑은 고딕"


    ' ,mp4 오타 .mp4로 수정
    ActiveDocument.Tables(1).Cell(Row:=11, Column:=3).Range.Select
    Selection.Find.ClearFormatting
    Selection.Find.Replacement.ClearFormatting
    With Selection.Find
        .Text = ",mp4"
        .Replacement.Text = ".mp4"
        .Forward = True
        .Wrap = wdFindStop
        .Format = False
        .MatchCase = False
        .MatchWholeWord = False
        .MatchByte = False
        .CorrectHangulEndings = False
        .HanjaPhoneticHangul = False
        .MatchAllWordForms = False
        .MatchSoundsLike = False
        .MatchWildcards = False
    End With
    Selection.Find.Execute Replace:=wdReplaceAll

    ' 타임코드 뒤에 공백+ 제거
    ActiveDocument.Tables(1).Cell(Row:=11, Column:=3).Range.Select
    Selection.Find.ClearFormatting
    Selection.Find.Replacement.ClearFormatting
    With Selection.Find
        .Text = "([0-9][0-9]:[0-9][0-9])( {2,})(?)"
        .Replacement.Text = "\1 \3"
        .Forward = True
        .Wrap = wdFindStop
        .Format = False
        .MatchCase = False
        .MatchWholeWord = False
        .MatchByte = False
        .CorrectHangulEndings = False
        .HanjaPhoneticHangul = False
        .MatchAllWordForms = False
        .MatchSoundsLike = False
        .MatchWildcards = True
    End With
    Selection.Find.Execute Replace:=wdReplaceAll

    ' [ 자막] 시작 공백 제거
    ActiveDocument.Tables(1).Cell(Row:=11, Column:=3).Range.Select
    Selection.Find.ClearFormatting
    Selection.Find.Replacement.ClearFormatting
    With Selection.Find
        .Text = "([0-9][0-9]):([0-9][0-9]) (\[)[ ]"
        .Replacement.Text = "\1:\2 \3"
        .Forward = True
        .Wrap = wdFindStop
        .Format = False
        .MatchCase = False
        .MatchWholeWord = False
        .MatchByte = False
        .CorrectHangulEndings = False
        .HanjaPhoneticHangul = False
        .MatchAllWordForms = False
        .MatchSoundsLike = False
        .MatchWildcards = True
    End With
    Selection.Find.Execute Replace:=wdReplaceAll

    ' [자막 ], [중요 자막] 중간&끝 공백 제거
    ActiveDocument.Tables(1).Cell(Row:=11, Column:=3).Range.Select
    Selection.Find.ClearFormatting
    Selection.Find.Replacement.ClearFormatting
    With Selection.Find
        .Text = "([0-9][0-9]):([0-9][0-9]) (\[??)[ ]"
        .Replacement.Text = "\1:\2 \3"
        .Forward = True
        .Wrap = wdFindStop
        .Format = False
        .MatchCase = False
        .MatchWholeWord = False
        .MatchByte = False
        .CorrectHangulEndings = False
        .HanjaPhoneticHangul = False
        .MatchAllWordForms = False
        .MatchSoundsLike = False
        .MatchWildcards = True
    End With
    Selection.Find.Execute Replace:=wdReplaceAll

    ' [중요자막 ] 끝 공백 제거
    ActiveDocument.Tables(1).Cell(Row:=11, Column:=3).Range.Select
    Selection.Find.ClearFormatting
    Selection.Find.Replacement.ClearFormatting
    With Selection.Find
        .Text = "([0-9][0-9]:[0-9][0-9] \[?{1,4}) (\])"
        .Replacement.Text = "\1\2"
        .Forward = True
        .Wrap = wdFindStop
        .Format = False
        .MatchCase = False
        .MatchWholeWord = False
        .MatchByte = False
        .CorrectHangulEndings = False
        .HanjaPhoneticHangul = False
        .MatchAllWordForms = False
        .MatchSoundsLike = False
        .MatchWildcards = True
    End With
    Selection.Find.Execute Replace:=wdReplaceAll

    '타임코드-자막 사이 공백 없을 시 공백 삽입
    ActiveDocument.Tables(1).Cell(Row:=11, Column:=3).Range.Select
    Selection.Find.ClearFormatting
    Selection.Find.Replacement.ClearFormatting
    With Selection.Find
        .Text = "([0-9][0-9]:[0-9][0-9])([! ])"
        .Replacement.Text = "\1 \2"
        .Forward = True
        .Wrap = wdFindContinue
        .Format = False
        .MatchCase = False
        .MatchWholeWord = False
        .MatchByte = False
        .CorrectHangulEndings = False
        .HanjaPhoneticHangul = False
        .MatchAllWordForms = False
        .MatchSoundsLike = False
        .MatchWildcards = True
    End With
    Selection.Find.Execute Replace:=wdReplaceAll


    ' ----로 클립 별 단 나누기 제거
    ActiveDocument.Tables(1).Cell(Row:=11, Column:=3).Range.Select
    Selection.Find.ClearFormatting
    Selection.Find.Replacement.ClearFormatting
    With Selection.Find
        .Text = "^13-@(^13)"
        .Replacement.Text = "\1"
        .Forward = True
        .Wrap = wdFindStop
        .Format = False
        .MatchCase = False
        .MatchWholeWord = False
        .MatchByte = False
        .CorrectHangulEndings = False
        .HanjaPhoneticHangul = False
        .MatchAllWordForms = False
        .MatchSoundsLike = False
        .MatchWildcards = True
    End With
    Selection.Find.Execute Replace:=wdReplaceAll


    '줄바꿈마다 /!/ 삽입
    ActiveDocument.Tables(1).Cell(Row:=11, Column:=3).Range.Select
    Selection.Find.ClearFormatting
    Selection.Find.Replacement.ClearFormatting
    With Selection.Find
        .Text = "(^13)"
        .Replacement.Text = "/!/\1"
        .Forward = True
        .Wrap = wdFindStop
        .Format = False
        .MatchCase = False
        .MatchWholeWord = False
        .MatchByte = False
        .CorrectHangulEndings = False
        .HanjaPhoneticHangul = False
        .MatchAllWordForms = False
        .MatchSoundsLike = False
        .MatchWildcards = True
    End With
    Selection.Find.Execute Replace:=wdReplaceAll

    '줄바꿈 다음에 다음 대본 아닐 때 줄 합치기
    ActiveDocument.Tables(1).Cell(Row:=11, Column:=3).Range.Select
    Selection.Find.ClearFormatting
    Selection.Find.Replacement.ClearFormatting
    With Selection.Find
        .Text = "(\/\!\/)(^13)([!0-9][!0-9][!\:][!0-9][!0-9])"
        .Replacement.Text = "\1 \3"
        .Forward = True
        .Wrap = wdFindStop
        .Format = False
        .MatchCase = False
        .MatchWholeWord = False
        .MatchByte = False
        .CorrectHangulEndings = False
        .HanjaPhoneticHangul = False
        .MatchAllWordForms = False
        .MatchSoundsLike = False
        .MatchWildcards = True
    End With
    Selection.Find.Execute Replace:=wdReplaceAll

    '필요없는 /!/ 제거1
    ActiveDocument.Tables(1).Cell(Row:=11, Column:=3).Range.Select
    Selection.Find.ClearFormatting
    Selection.Find.Replacement.ClearFormatting
    With Selection.Find
        .Text = "(\/\!\/)(^13)"
        .Replacement.Text = "\2"
        .Forward = True
        .Wrap = wdFindContinued
        .Format = False
        .MatchCase = False
        .MatchWholeWord = False
        .MatchByte = False
        .CorrectHangulEndings = False
        .HanjaPhoneticHangul = False
        .MatchAllWordForms = False
        .MatchSoundsLike = False
        .MatchWildcards = True
    End With
    Selection.Find.Execute Replace:=wdReplaceAll


    '필요없는 /!/ 제거2 (/!/ 뒤에 공백 생길 경우)
    ActiveDocument.Tables(1).Cell(Row:=11, Column:=3).Range.Select
    Selection.Find.ClearFormatting
    Selection.Find.Replacement.ClearFormatting
    With Selection.Find
        .Text = "(\/\!\/)([ ])(^13)"
        .Replacement.Text = "\2"
        .Forward = True
        .Wrap = wdFindStop
        .Format = False
        .MatchCase = False
        .MatchWholeWord = False
        .MatchByte = False
        .CorrectHangulEndings = False
        .HanjaPhoneticHangul = False
        .MatchAllWordForms = False
        .MatchSoundsLike = False
        .MatchWildcards = True
    End With
    Selection.Find.Execute Replace:=wdReplaceAll

    '필요없는 줄 바꿈 모두 제거
    ActiveDocument.Tables(1).Cell(Row:=11, Column:=3).Range.Select
    Selection.Find.ClearFormatting
    Selection.Find.Replacement.ClearFormatting
    With Selection.Find
        .Text = "(^13{2,})"
        .Replacement.Text = "^p"
        .Forward = True
        .Wrap = wdFindContinue
        .Format = False
        .MatchCase = False
        .MatchWholeWord = False
        .MatchByte = False
        .CorrectHangulEndings = False
        .HanjaPhoneticHangul = False
        .MatchAllWordForms = False
        .MatchSoundsLike = False
        .MatchWildcards = True
    End With
    Selection.Find.Execute Replace:=wdReplaceAll


    '표를 텍스트로 변환하면서 생성된 탭 및 기타 탭 제거
    Selection.Find.ClearFormatting
    Selection.Find.Replacement.ClearFormatting
    With Selection.Find
        .Text = "^t"
        .Replacement.Text = "  "
        .Forward = True
        .Wrap = wdFindContinue
        .Format = False
        .MatchCase = False
        .MatchWholeWord = False
        .MatchByte = False
        .CorrectHangulEndings = True
        .HanjaPhoneticHangul = False
        .MatchWildcards = False
        .MatchSoundsLike = False
        .MatchAllWordForms = False
    End With
    Selection.Find.Execute Replace:=wdReplaceAll


    '너무 넓은 공백 간소화
    ActiveDocument.Tables(1).Cell(Row:=11, Column:=3).Range.Select
    Selection.Find.ClearFormatting
    Selection.Find.Replacement.ClearFormatting
    With Selection.Find
        .Text = " {3,100}"
        .Replacement.Text = " "
        .Forward = True
        .Wrap = wdFindContinue
        .Format = False
        .MatchCase = False
        .MatchWholeWord = False
        .MatchByte = False
        .CorrectHangulEndings = False
        .HanjaPhoneticHangul = False
        .MatchAllWordForms = False
        .MatchSoundsLike = False
        .MatchWildcards = True
    End With
    Selection.Find.Execute Replace:=wdReplaceAll

    '줄바꿈문자(/!/) 앞 공백 만들기
    ActiveDocument.Tables(1).Cell(Row:=11, Column:=3).Range.Select
    Selection.Find.ClearFormatting
    Selection.Find.Replacement.ClearFormatting
    With Selection.Find
        .Text = "([! ])(\/\!\/)"
        .Replacement.Text = "\1 \2"
        .Forward = True
        .Wrap = wdFindStop
        .Format = False
        .MatchCase = False
        .MatchWholeWord = False
        .MatchByte = False
        .CorrectHangulEndings = False
        .HanjaPhoneticHangul = False
        .MatchAllWordForms = False
        .MatchSoundsLike = False
        .MatchWildcards = True
    End With
    Selection.Find.Execute Replace:=wdReplaceAll

    '들여쓰기 제거
    ActiveDocument.Tables(1).Cell(Row:=11, Column:=3).Range.Select
    remove_indents
    remove_all_the_first_line_indent_spaces

    'mp4뒤 공백 제거
    ActiveDocument.Tables(1).Cell(Row:=11, Column:=3).Range.Select
    Selection.Find.ClearFormatting
    Selection.Find.Replacement.ClearFormatting
    With Selection.Find
        .Text = "(^13[0-9]{1,}\-[0-9]{1,}\-[0-9]{1,}?mp4)([ ]{1,})(^13)"
        .Replacement.Text = "\1\3"
        .Forward = True
        .Wrap = wdFindContinue
        .Format = False
        .MatchCase = False
        .MatchWholeWord = False
        .MatchByte = False
        .CorrectHangulEndings = True
        .HanjaPhoneticHangul = False
        .MatchWildcards = True
        .MatchSoundsLike = False
        .MatchAllWordForms = False
    End With
    Selection.Find.Execute Replace:=wdReplaceAll

    '클립 분리용외 mp4 삭제
    ActiveDocument.Tables(1).Cell(Row:=11, Column:=3).Range.Select
    Selection.Find.ClearFormatting
    Selection.Find.Replacement.ClearFormatting
    With Selection.Find
        .Text = "([!^13][0-9]{1,}\-[0-9]{1,}\-[0-9]{1,})(?mp4[!^13])"
        .Replacement.Text = "\1 "
        .Forward = True
        .Wrap = wdFindContinue
        .Format = False
        .MatchCase = False
        .MatchWholeWord = False
        .MatchByte = False
        .CorrectHangulEndings = True
        .HanjaPhoneticHangul = False
        .MatchWildcards = True
        .MatchSoundsLike = False
        .MatchAllWordForms = False
    End With
    Selection.Find.Execute Replace:=wdReplaceAll

    '엑셀,텍스트파일에서 물음표로 되는 공백 특수기호(ChrW(8198) 삭제 - 수식텍스트변환할때생성됨
    ActiveDocument.Tables(1).Cell(Row:=11, Column:=3).Range.Select
    Selection.Find.ClearFormatting
    Selection.Find.Replacement.ClearFormatting
    With Selection.Find
        .Text = ChrW(8198)
        .Replacement.Text = ""
        .Forward = True
        .Wrap = wdFindContinue
        .Format = False
        .MatchCase = False
        .MatchWholeWord = False
        .MatchByte = False
        .CorrectHangulEndings = True
        .HanjaPhoneticHangul = False
        .MatchWildcards = False
        .MatchSoundsLike = False
        .MatchAllWordForms = False
    End With
    Selection.Find.Execute Replace:=wdReplaceAll

    ' 내용 선택&복사
    ActiveDocument.Tables(1).Cell(Row:=11, Column:=3).Range.Select
    Selection.Copy


End Sub

'들여쓰기 제거 명령 실행을 위한 모듈
Sub remove_indents()
    With Selection.ParagraphFormat
    .CharacterUnitLeftIndent = 0
    .CharacterUnitRightIndent = 0
    .CharacterUnitFirstLineIndent = 0
    .LeftIndent = CentimetersToPoints(0)
    .RightIndent = CentimetersToPoints(0)
    .FirstLineIndent = CentimetersToPoints(0)
End With
End Sub
Sub remove_all_the_first_line_indent_spaces()
    Dim i As Paragraph, n As Long
    Application.ScreenUpdating = False 'close screen and refresh
    For Each i In ActiveDocument.Paragraphs 'cycling in the pragraphs of the active document
    For n = 1 To i.Range.Characters.Count
    If i.Range.Characters(1).Text = " " Or i.Range.Characters(1).Text = "　" Or i.Range.Characters(1).Text = Chr(9) Then
    i.Range.Characters(1).Delete
    Else: Exit For
    End If
    Next n
    Next
    Application.ScreenUpdating = True 'restore screen and refresh
End Sub
